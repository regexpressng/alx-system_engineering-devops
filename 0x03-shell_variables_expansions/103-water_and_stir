#!/bin/bash
r="";n=$(( $(for((i=0;i<${#WATER};i++));do c="${WATER:$i:1}";echo $(( $(expr index "water" "$c") -1 ));done | awk '{s=s*5+$1}END{print s}') + $(for((i=0;i<${#STIR};i++));do c="${STIR:$i:1}";echo $(( $(expr index "stir" "$c") -1 ));done | awk '{s=s*4+$1}END{print s}') )); while [ $n -gt 0 ]; do r="${r}${BESTCHOL:$((n%${#BESTCHOL})):1}"; n=$(( n/${#BESTCHOL} )); done; echo $(echo $r | rev)
